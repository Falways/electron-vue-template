{
  "name": "electron-vue-template",
  "version": "0.0.0",
  "author": "sky <https://github.com/umbrella22>",
  "description": "An electron-vue project",
  "license": "MIT",
  "main": "./dist/electron/main/main.js",
  "scripts": {
    "dev": "tsx .electron-vue/dev-runner.ts",
    "dev:sit": "tsx .electron-vue/dev-runner.ts -m sit",
    "build": "tsx .electron-vue/build.ts && electron-builder",
    "build:win32": "tsx .electron-vue/build.ts && electron-builder --win  --ia32",
    "build:win64": "tsx .electron-vue/build.ts && electron-builder --win  --x64",
    "build:mac": "tsx .electron-vue/build.ts && electron-builder --mac",
    "build:dir": "tsx .electron-vue/build.ts && electron-builder --dir",
    "build:clean": "tsx .electron-vue/build.ts --clean",
    "build:web": "tsx .electron-vue/build.ts --target web",
    "pack:resources": "tsx .electron-vue/hot-updater.ts",
    "dep:upgrade": "yarn upgrade-interactive --latest",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "electron-updater": "^6.3.4"
  },
  "devDependencies": {
    "@ikaros-cli/prettier-config": "^0.0.1",
    "@ikaros-cli/stylelint-config": "^0.0.1",
    "@rspack/cli": "^1.0.5",
    "@rspack/core": "^1.0.5",
    "@rspack/dev-server": "^1.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.5.5",
    "adm-zip": "^0.5.16",
    "axios": "^1.7.7",
    "cfonts": "^3.3.0",
    "chalk": "^5.3.0",
    "css-loader": "^7.1.2",
    "del": "^7.1.0",
    "dotenv": "^16.4.5",
    "electron": "^32.1.0",
    "electron-builder": "^25.0.5",
    "electron-devtools-vendor": "^3.0.0",
    "fs-extra": "^11.2.0",
    "listr2": "^8.2.4",
    "minimist": "^1.2.8",
    "pinia": "2.2.2",
    "portfinder": "^1.0.32",
    "sass-embedded": "^1.79.1",
    "sass-loader": "^16.0.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vue": "^3.5.6",
    "vue-loader": "^17.4.2",
    "vue-router": "^4.4.5",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.94.0"
  }
}
